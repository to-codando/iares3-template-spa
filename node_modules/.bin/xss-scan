#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/dev/Projetos/meta-iares-2/node_modules/.pnpm/@kitajs+ts-html-plugin@4.1.0_@kitajs+html@3.1.2_typescript@5.6.3/node_modules/@kitajs/ts-html-plugin/dist/node_modules:/home/dev/Projetos/meta-iares-2/node_modules/.pnpm/@kitajs+ts-html-plugin@4.1.0_@kitajs+html@3.1.2_typescript@5.6.3/node_modules/@kitajs/ts-html-plugin/node_modules:/home/dev/Projetos/meta-iares-2/node_modules/.pnpm/@kitajs+ts-html-plugin@4.1.0_@kitajs+html@3.1.2_typescript@5.6.3/node_modules/@kitajs/node_modules:/home/dev/Projetos/meta-iares-2/node_modules/.pnpm/@kitajs+ts-html-plugin@4.1.0_@kitajs+html@3.1.2_typescript@5.6.3/node_modules:/home/dev/Projetos/meta-iares-2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/dev/Projetos/meta-iares-2/node_modules/.pnpm/@kitajs+ts-html-plugin@4.1.0_@kitajs+html@3.1.2_typescript@5.6.3/node_modules/@kitajs/ts-html-plugin/dist/node_modules:/home/dev/Projetos/meta-iares-2/node_modules/.pnpm/@kitajs+ts-html-plugin@4.1.0_@kitajs+html@3.1.2_typescript@5.6.3/node_modules/@kitajs/ts-html-plugin/node_modules:/home/dev/Projetos/meta-iares-2/node_modules/.pnpm/@kitajs+ts-html-plugin@4.1.0_@kitajs+html@3.1.2_typescript@5.6.3/node_modules/@kitajs/node_modules:/home/dev/Projetos/meta-iares-2/node_modules/.pnpm/@kitajs+ts-html-plugin@4.1.0_@kitajs+html@3.1.2_typescript@5.6.3/node_modules:/home/dev/Projetos/meta-iares-2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/@kitajs+ts-html-plugin@4.1.0_@kitajs+html@3.1.2_typescript@5.6.3/node_modules/@kitajs/ts-html-plugin/dist/cli.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/@kitajs+ts-html-plugin@4.1.0_@kitajs+html@3.1.2_typescript@5.6.3/node_modules/@kitajs/ts-html-plugin/dist/cli.js" "$@"
fi
