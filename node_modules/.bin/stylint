#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/dev/Projetos/meta-iares-2/node_modules/.pnpm/stylint@2.0.0/node_modules/stylint/bin/node_modules:/home/dev/Projetos/meta-iares-2/node_modules/.pnpm/stylint@2.0.0/node_modules/stylint/node_modules:/home/dev/Projetos/meta-iares-2/node_modules/.pnpm/stylint@2.0.0/node_modules:/home/dev/Projetos/meta-iares-2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/dev/Projetos/meta-iares-2/node_modules/.pnpm/stylint@2.0.0/node_modules/stylint/bin/node_modules:/home/dev/Projetos/meta-iares-2/node_modules/.pnpm/stylint@2.0.0/node_modules/stylint/node_modules:/home/dev/Projetos/meta-iares-2/node_modules/.pnpm/stylint@2.0.0/node_modules:/home/dev/Projetos/meta-iares-2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../stylint/bin/stylint" "$@"
else
  exec node  "$basedir/../stylint/bin/stylint" "$@"
fi
